{
  "name": "wavematch",
  "license": "MIT",
  "description": "JS Pattern Matching",
  "version": "1.0.6",
  "main": "lib/wavematch.js",
  "module": "es/wavematch.js",
  "files": [
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "lint": "eslint src test lib es",
    "lint:check": "eslint --print-config . | eslint-config-prettier-check",
    "clean": "rm -rf lib es",
    "test": "yarn build && NODE_ENV=development mocha --no-colors",
    "build": "rollup --silent --config",
    "build:watch": "rollup --silent --config --watch",
    "format": "prettier --write {src,lib,es}/**.js",
    "prepublish": "yarn clean && yarn build && yarn lint && yarn format"
  },
  "repository": {
    "type": "git",
    "url": "https//github.com/chrisisler/wavematch.git"
  },
  "bugs": {
    "url": "https//github.com/chrisisler/wavematch/issues"
  },
  "keywords": [
    "functional",
    "pattern",
    "match",
    "wavematch",
    "case",
    "branch",
    "patterns",
    "matches",
    "rematch"
  ],
  "author": "chrisisler <christopherisler1@gmail.com> (https://github.com/chrisisler)",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-preset-flow": "^6.23.0",
    "cross-env": "^5.1.4",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.66.0",
    "mocha": "^5.0.4",
    "prettier": "1.11.1",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "dependencies": {
    "fast-deep-equal": "^2.0.1",
    "json5": "^1.0.1",
    "parse-function": "^5.2.9"
  }
}
