{
  "name": "wavematch",
  "license": "MIT",
  "description": "Pattern matching control flow mechanism.",
  "version": "1.2.0",
  "main": "lib/wavematch.js",
  "module": "es/wavematch.js",
  "files": [
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "lint": "eslint src test",
    "lint:check": "eslint --print-config . | eslint-config-prettier-check",
    "clean": "rm -rf lib es",
    "test": "yarn build && NODE_ENV=development mocha --no-colors",
    "build": "rollup --silent --config",
    "build:demo": "yarn clean && yarn build",
    "build:watch": "rollup --silent --config --watch",
    "prepare": "yarn clean && yarn format && yarn lint && yarn test",
    "format": "prettier --write src/**.ts",
    "prepublish": "yarn clean && yarn build && yarn lint && yarn format"
  },
  "repository": {
    "type": "git",
    "url": "https//github.com/chrisisler/wavematch.git"
  },
  "bugs": {
    "url": "https//github.com/chrisisler/wavematch/issues"
  },
  "keywords": [
    "functional",
    "pattern",
    "match",
    "wavematch",
    "control",
    "condition",
    "conditional",
    "case",
    "branch",
    "patterns",
    "matches",
    "rematch"
  ],
  "author": "chrisisler <christopherisler1@gmail.com> (https://github.com/chrisisler)",
  "devDependencies": {
    "@types/json5": "^0.0.30",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "cross-env": "^5.1.4",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "mocha": "^5.0.4",
    "prettier": "1.11.1",
    "rollup": "^0.64.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^3.0.0",
    "tslib": "^1.10.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "fast-deep-equal": "^2.0.1",
    "json5": "^2.1.1",
    "parse-function": "^5.2.11"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "semi": false,
    "parser": "typescript",
    "arrowParens": "avoid"
  }
}
